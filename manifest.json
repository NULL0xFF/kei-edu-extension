{
  "manifest_version": 3,
  "name": "KEI Edu Admin Tools",
  "version": "0.0.0",
  "description": "A modern extension for managing Korea Environment Institute E-Learning Platform",
  "permissions": [
    "storage",
    "unlimitedStorage"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://cyber-admin.kei.re.kr/*",
        "https://cyber-admin.kei.re.kr/*"
      ],
      "js": [
        "content-script.bundle.js"
      ],
      "run_at": "document_idle"
    }
  ],
  "host_permissions": [
    "http://cyber-admin.kei.re.kr/*",
    "https://cyber-admin.kei.re.kr/*"
  ],
  "author": "Myoung Ha, Ji",
  "homepage_url": "https://github.com/NULL0xff/kei-edu-extension"
}