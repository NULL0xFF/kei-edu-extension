{
    "name": "KEI Admin Tools",
    "description": "Admin",
    "version": "1.0",
    "manifest_version": 3,
    "permissions": [
        "tabs"
    ],
    "host_permissions": [
        "http://cyber-admin.kei.re.kr/user/member/memberList.do",
        "http://cyber-admin.kei.re.kr/cmmn/main.do"
    ],
    "content_scripts": [
        {
            "matches": [
                "http://cyber-admin.kei.re.kr/user/member/memberList.do",
                "http://cyber-admin.kei.re.kr/cmmn/main.do"
            ],
            "js": [
                "js/jquery-2.1.1.js",
                "js/content_script.js"
            ]
        }
    ],
    "web_accessible_resources": [
        {
            "resources": [
                "js/dashboard.js",
                "js/member.js",
                "js/shared.js"
            ],
            "matches": [
                "<all_urls>"
            ]
        }
    ]
}