{
  "manifest_version": 3,
  "name": "KEI Edu Admin Tools",
  "version": "0.0.0",
  "description": "A extension for managing Korea Environment Institute E-Learning Platform",
  "permissions": [
    "storage",
    "unlimitedStorage",
    "scripting"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://cyber-admin.kei.re.kr/*",
        "https://cyber-admin.kei.re.kr/*"
      ],
      "js": [
        "contentScript.bundle.js"
      ]
    }
  ],
  "host_permissions": [
    "http://cyber-admin.kei.re.kr/*",
    "https://cyber-admin.kei.re.kr/*"
  ]
}
